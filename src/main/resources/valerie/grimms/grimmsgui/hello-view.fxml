<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="vbox" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="valerie.grimms.grimmsgui.GrimmsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>


    <TextFlow prefHeight="5.0" prefWidth="408.0" textAlignment="CENTER"> <Label fx:id="welcomeText" alignment="CENTER" prefHeight="43.0" prefWidth="408.0" style="-fx-font-weight: bold" text="This is a project based on the fairy tales of the German Grimm Brothers." textAlignment="CENTER" /> </TextFlow>
   <Label fx:id="labelInstructions" alignment="CENTER" text="Select a fairy tale. This will generate and open a Word document with the story's text:" textAlignment="CENTER" />
   <RadioButton id="frogKing" fx:id="frogKing" mnemonicParsing="false" onAction="#onFrogKingClick" prefHeight="18.0" prefWidth="121.0" text="The Frog King">
      <toggleGroup>
         <ToggleGroup fx:id="group1" />
      </toggleGroup>
      <VBox.margin>
         <Insets />
      </VBox.margin></RadioButton>
   <RadioButton id="rapunzel" fx:id="rapunzel" layoutX="160.0" layoutY="68.0" mnemonicParsing="false" onAction="#onRapunzelClick" prefHeight="18.0" prefWidth="122.0" text="Rapunzel" toggleGroup="$group1" />
   <RadioButton id="hanselAndGretel" fx:id="hanselAndGretel" layoutX="188.0" layoutY="106.0" mnemonicParsing="false" onAction="#onHanselAndGretelClick" prefHeight="18.0" prefWidth="126.0" text="Hansel and Gretel" toggleGroup="$group1" />
</VBox>
